{
  "version": 2,
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat.ts",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/supabase",
      "dest": "/api/supabase.ts",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/auth",
      "dest": "/api/auth.ts",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/chatbot-widget.umd.js",
      "dest": "/dist/chatbot-widget.umd.js"
    },
    {
      "src": "/chatbot-widget.es.js",
      "dest": "/dist/chatbot-widget.es.js"
    },
    {
      "src": "/style.css",
      "dest": "/dist/style.css"
    },
    {
      "src": "/(.*)",
      "dest": "/test-site.html"
    }
  ]
}
