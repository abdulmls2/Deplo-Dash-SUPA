{
  "version": 2,
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat.ts",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/widget",
      "dest": "/api/widget.ts",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}
